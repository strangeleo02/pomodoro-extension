{
    "manifest_version": 3,
    "name": "Customizable Pomodoro Timer",
    "version": "1.0.0",
    "description": "A Pomodoro timer with customizable work and break durations.",
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/icon16.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
      }
    },
    "permissions": [
      "storage",
      "notifications"
    ],
   "icons": {
     "16": "images/icon16.png",
     "48": "images/icon48.png",
     "128": "images/icon128.png"
     }
  }